package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		Queue que = new Queue(20);
		que.enqueue(1);
		que.enqueue(21);
		
		que.dump();
		System.out.println("---------------");
		
		que.dequeue();
		que.dump();
	}
	
	
}

class Queue {
	private int max;		// 큐 용량
	private int num;		// 현재 데이터 수
	private int[] que;		// 큐 본체
	
	public Queue(int capacity) {
		num = 0;
		max = capacity;
		que = new int[max];
	}
	
	
	public void enqueue(int n) {
		if (max <= num) {
			System.out.println("데이터 꽉참요");
			return;
		} else {
			que[num++] = n;
		}
	}
	
	public int dequeue() {
		if (num <= 0) {
			return -1;
		} else {
			int result = que[--num];
			
			for (int i = num - 1; i >= 0; i--) {
				que[i] = que[i+1];
			}
			
			return result; 
		}
		
	}
	
	public void dump() {
		if (num <= 0) {
			System.out.println("데이터가");
		} else {
			for (int i = 0; i < num; i++) {
				System.out.println(que[i] + " ");
			}
		}
	}
	
	
	
}
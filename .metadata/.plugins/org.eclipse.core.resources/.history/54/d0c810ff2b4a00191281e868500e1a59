package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		
		System.out.println(med3(2, 1, 4));
	}
	
	// 세 값의 중앙 값 구하기
	public static int med3(int a, int b, int c) {
		int val = 0;
		
		
		if (a > b) {
			if (b > c) {
				val = c;
			} else {
				val = b;
			}
		} else if (a > c) {
			
		}
		
		
		return val;
	}
	
	

}

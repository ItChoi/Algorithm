package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		int nx = 8;
		int[] x = new int[] {8,1,4,2,7,6,3,5};
		
		shellSort(x, nx);
		
		for (int test : x) {
			System.out.println("Á¤·Ä: " + test);
		}
	}
	
	static void shellSort(int[] a, int n) {
		
		for (int i = n / 2; i > 0; i /= 2) {
			int tmp = a[i];
			System.out.println("tmp: " + tmp);
			for (int j = 0; j < n && a[j] > tmp; j += i) {
				System.out.println("tmp: " + tmp);
				a[i] = a[j]; 
				a[j] = tmp;
			}
			break;
			
		}
		
	}
	
}
package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		int n = 21;
		int i = 1;
		int sum = 0;
		
		sum = (i + n) * n / 2;
		
		int sumTest = 0;
		for (int j = 1; j <= 21; j++) {
			sumTest += j;
		}
		
		System.out.println(sum);
		System.out.println(sumTest);
	}
	

}

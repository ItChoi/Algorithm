package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		
		
	}
	
	
	// swap
	static void swap(int[] a, int idx1, int idx2) {
		int temp = a[idx1];
		a[idx1] = a[idx2];
		a[idx2] = temp;
	}
	
	// 버블 정렬
	static void bubbleSort(int[] a, int n) {
		for (int i = 0; i < n - 1; i++) {
			for (int j = i; i < n - 1; j++) {
				if (a[j] > a[j + 1]) {
					swap(a, j + 1, j);
				}
			}
		}
	}
	
}
package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		int nx = 8;
		int[] x = new int[] {8,1,4,2,7,6,3,5};
		
		shellSort(x, nx);
	}
	
	static void shellSort(int[] a, int n) {
		
		for (int h = n / 2; h > 0; h /= 2) {
			for (int i = h; h < n; i++) {
				int j;
				int tmp = a[i];
				for (j = i - h; j >= 0 && a[j] > tmp; j-=h) {
					 
				}
			}
		}
		
	}
	
}
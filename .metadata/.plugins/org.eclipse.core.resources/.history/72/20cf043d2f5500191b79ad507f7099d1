package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		
		int x = 4;
		
		recur(x);
		
	}
	
	static void recur(int n) {
		int temp = 0;
		while (true) {
			if (n > 0) {
				temp = n;
				n = n - 1;
				continue;
			}
			
			if (temp != 0) {
				n = temp;
				System.out.println("n: " + n);
				n = n - 2;
				continue;
			}
		}
	}
}
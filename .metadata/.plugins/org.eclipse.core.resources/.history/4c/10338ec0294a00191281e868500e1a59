package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		
	}
	// Q1) 네 값의 최대 값을 구하는 max4 메서드를 작성하세요.
	public static int max4(int a, int b, int c, int d) {
		int max = a;
		
		if (b > max) {
			max = b;
		} else if (c > max) {
			max = c;
		} else if (d > max) {
			max = d;
		}
		
		return max;
	}

}

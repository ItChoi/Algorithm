package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		
		System.out.println(max4(1, 2, 1, 5));
	}
	// Q2) 세 값의 최솟값을 구하는 min3 메서드를 작성하세요.
	public static int max4(int a, int b, int c, int d) {
		int max = a;
		
		if (b > max) {
			max = b;
		} else if (c > max) {
			max = c;
		} else if (d > max) {
			max = d;
		}
		
		return max;
	}

}

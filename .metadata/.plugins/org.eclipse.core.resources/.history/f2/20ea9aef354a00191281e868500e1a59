package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		
		System.out.println(med3(2, 1, 4));
	}
	
	// 세 값의 중앙 값 구하기
	public static int med3(int d, int dd, int ddd) {
		int val = 0;
		
		int a = 2; int b = 3; int c = 4;
		
		
		if (a >= b) {
			
			if (b >= c) {
				val = b;
			} else if (a > c) {
				val = c;
			} else {
				val = a;
			}
			
		} else if(b > c) {
			
			if (a > c) {
				val = a;
			} else {
				val = c;
			}
			
		} else {
			val = b;
		}
		
		
		
		System.out.println("val: " + val);
		
		
		return val;
	}
	
	

}

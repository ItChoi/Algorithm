package test;

public class TestAlgorithm {

	public static void main(String[] args) {
		
		System.out.println(med3(2, 1, 4));
	}
	
	// 세 값의 중앙 값 구하기
	public static int med3(int d, int dd, int ddd) {
		int val = 0;
		
		int a = 1, int b = 2, int c = 3;
	
		
		
		if (a > b) {
			
			if (b > c) {
				val = c;
			} else {
				val = b;
			}
			
		} else if (a > c) {
			
			if (b > c) {
				val = b;
			} else {
				val = c;
			}
			
		} 
		
		
		return val;
	}
	
	

}
